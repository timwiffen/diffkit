		<constructor-arg index="0" value="COLUMN5" />
		<constructor-arg index="1" value="5" />
		<constructor-arg index="2" value="TIME" />
		<constructor-arg index="3" value="0" />
		<constructor-arg index="4" value="true" />
	</bean>

	<bean id="rhs.column6" class="org.diffkit.db.DKDBColumn">
		<constructor-arg index="0" value="COLUMN6" />
